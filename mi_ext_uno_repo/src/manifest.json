{
    "manifest_version": 3,
    "name": "AutoAgenda Consular Visa",
    "version": "1.0",
    "description": "Extensión para automatizar la programación de citas.",
    "permissions": [
      "storage",
      "scripting",
      "activeTab",
      "declarativeContent",
      "webRequest",
      "webRequestBlocking",
      "host_permissions"
    ],
    "host_permissions": [
      "https://ais.usvisa-info.com/*"
    ],
    "background": {
      "service_worker": "src/background.js" 
    },
    "content_scripts": [
      {
        "matches": ["https://ais.usvisa-info.com/es-mx/niv/*"],
        "js": ["src/content.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": "assd/images/128X128TL1.png" 
    },
    "icons": {
      "48": "assd/images/128X128TL1.png"  
    },
    "options_page": "src/options.html"
  }
  